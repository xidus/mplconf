{
  "figure": {
    "edgecolor": "black",
    "facecolor": "white",
    "dpi": 80,
    "figsize": [14, 8.75]
  },
  "savefig": {
    "edgecolor": "white",
    "pad_inches": 0.1,
    "bbox": "tight",
    "dpi": 300,
    "facecolor": "white"
  },
  "text": {
    "color": "black",
    "usetex": false
  },

  "image.cmap": "coolwarm",

  "axes.color_cycle": [
      "#9c9cdb",
      "#4d5585",
      "#b4cc6a",
      "#64783a",
      "#e3ba54",
      "#8c6d34"
  ],
  "axes": {
    "edgecolor": "black",
    "facecolor": "white",
    "axisbelow": false,
    "labelweight": "normal",
    "labelcolor": "black",
    "grid": true,
    "unicode_minus": true,
    "linewidth": 1.0,
    "labelsize": 14
  },
  "lines": {
    "linewidth": 1.0,
    "markeredgewidth": 0.5,
    "markersize": 6,
    "linestyle": "-",
    "marker": null
  },
  "xtick": {
    "color": "k",
    "labelsize": 11
  },
  "ytick": {
    "color": "k",
    "labelsize": 11
  },
  "grid": {
    "color": "black",
    "linewidth": 0.5,
    "linestyle": ":"
  },
  "font": {
    "serif": "Palatino",
    "family": "serif",
    "size": 18
  },
  "legend": {
    "fancybox": true,
    "fontsize": 11,
    "borderpad": 0.5,
    "shadow": false,
    "markerscale": 1.0,
    "numpoints": 1,
    "frameon": true
  }
}